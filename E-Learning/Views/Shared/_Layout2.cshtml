 
   

<!DOCTYPE html>
<html>

<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">

    <script src="~/Scripts/jquery-3.4.1.js"></script>
    <script src="~/Scripts/jquery-ui.js"></script>
    <link href="~/Content/SidebarCss/style2.css" rel="stylesheet" />
    <script src="~/Scripts/jquery-3.4.1.js"></script>
    <script src="~/Scripts/umd/popper.js"></script>
    <link href="~/Content/jquery-ui.css" rel="stylesheet" />


    <link href="~/Content/SidebarCss/style2.css" rel="stylesheet" />
    <link href="~/Content/bootstrap.css" rel="stylesheet" />

    <link href="~/Content/malihu-custom-scrollbar-plugin-master/malihu-custom-scrollbar-plugin-master/jquery.mCustomScrollbar.css" rel="stylesheet" />
    <link href="~/Content/Animate/animate.css" rel="stylesheet" />
    <script src="~/Content/toastr/toastr.min.js"></script>
    <link href="~/Content/toastr/toastr.css" rel="stylesheet" />
    <script src="~/Content/Sweetalaert/sweetalert.js"></script>
    @Scripts.Render("~/bundles/modernizr")
    <!-- Bootstrap CSS CDN -->
    <!-- Our Custom CSS -->
    <link href="~/Content/fontawesome-free-5.13.0-web/fontawesome-free-5.13.0-web/css/all.css" rel="stylesheet" />
    <link href="~/content/custome css/style4.css" rel="stylesheet" />
    <style>
 
    </style>
   
</head>

<body>

    <div class="wrapper">
        <!-- Sidebar  -->
       
        <nav id="sidebar" class="text-white">
            <div class="sidebar-header">
                <h3 class="font-weight-bold">E-Learning</h3>
            </div>

            <ul class="list-unstyled components">
                <li class="active">
                    <a href="#homeSubmenu"  class="border-0 bg-default text-white text-left btn btn-block btn-indigo">Home</a>
                    
                </li>
                <li>
                    <a href="#">About</a>
                </li>
                <li>
                    <a href="#pageSubmenu" data-toggle="collapse"  class="dropdown-toggle">Pages</a>
                    <ul class="collapse list-unstyled" id="pageSubmenu">
                        <li>
                            <a href="#">Page 1</a>
                        </li>
                        <li>
                            <a href="#">Page 2</a>
                        </li>
                        <li>
                            <a href="#">Page 3</a>
                        </li>
                    </ul>
                </li>
                <li>
                    <a href="#">Portfolio</a>
                </li>
                <li>
                    <a href="#">Contact</a>
                </li>
            </ul>
            <footer class="footer-copyright text-white">
                <p class="text-white">&copy; @DateTime.Now.Year - My ASP.NET Application</p>
            </footer>
        </nav>

        <!-- Page Content  -->
        <div id="content" class="shadow-lg">
            <section>
                @{
                    Html.RenderPartial("~/Views/Shared/_sidebarpage.cshtml");
                }
                

            </section>
            <section>
                <div class="p-2 m-2 bg-white  body-content">
                   
                    @RenderBody()


                </div>
            </section>


        </div>
    </div>
   
    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/popper")

    @Scripts.Render("~/bundles/bootstrap")

    @RenderSection("scripts", required: false)

    <!-- jQuery Custom Scroller CDN -->
    <script src="https://cdnjs.cloudflare.com/ajax/libs/malihu-custom-scrollbar-plugin/3.1.5/jquery.mCustomScrollbar.concat.min.js"></script>

    <script type="text/javascript">
        $(document).ready(function () {
            $("#sidebar").mCustomScrollbar({
                theme: "minimal"
            });

            $('#sidebarCollapse').on('click', function () {
                $('#sidebar, #content').delay(5000).toggleClass('active');
                $('.collapse.in').delay(5000).toggleClass('in');
                //$('#sidebar, #content').delay(1000).fadeToggle();
                $('a[aria-expanded=true]').attr('aria-expanded', 'false');
            });

            var img = $('#userimage').prop('src');
            
            $("#userimage").attr("src",'@Session["image"]');

        });
    </script>
</body>

</html>